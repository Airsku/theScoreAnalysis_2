<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D1F2FA"
    android:padding="10dp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <!--标题栏-->
            <RelativeLayout
                android:id="@+id/score_relative1"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@drawable/score"
                android:padding="5dp">
                <ImageButton
                    android:id="@+id/jumpToNext"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:scaleType="centerInside"
                    android:background="@drawable/score"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/double_right"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="具体成绩分析"
                    android:textColor="@color/black"
                    android:textSize="30dp"
                    android:layout_centerInParent="true"/>
                <ImageButton
                    android:id="@+id/jumpToLast"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:scaleType="centerInside"
                    android:background="@drawable/score"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/double_left"/>
            </RelativeLayout>


            <!--图框-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="200dp"
                        android:layout_height="200dp"
                        android:padding="5dp"
                        android:src="@drawable/circle" />
                    <TextView
                        android:id="@+id/total"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0.00"
                        android:textSize="40sp"
                        android:layout_centerInParent="true"
                        android:textColor="#138BA9"/>
                </RelativeLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/jidian"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="绩点:"
                                android:textColor="#000"
                                android:textSize="20sp"/>
                            <TextView
                                android:id="@+id/jdtext"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.0"
                                android:layout_marginLeft="5dp"
                                android:textSize="20sp"
                                android:textColor="#138BA9"
                                android:layout_toRightOf="@+id/jidian"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/zongxuefeng"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="总学分:"
                                android:textColor="#000"
                                android:layout_marginTop="5dp"
                                android:textSize="20sp"
                                android:layout_below="@+id/jidian"/>
                            <TextView
                                android:id="@+id/totalSec"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.00"
                                android:layout_marginTop="5dp"
                                android:textSize="20sp"
                                android:textColor="#138BA9"
                                android:layout_below="@+id/jidian"
                                android:layout_toRightOf="@+id/zongxuefeng"/>
                        </LinearLayout>
                 </LinearLayout>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    >
                    <TextView
                        android:id="@+id/wendingxing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="稳定性"
                        android:textColor="#000"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="10dp"
                        android:textSize="20sp"/>
                    <TextView
                        android:id="@+id/wdJudge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="未知"
                        android:textColor="#138BA9"
                        android:layout_below="@+id/wendingxing"
                        android:layout_centerHorizontal="true"
                        android:textSize="30sp"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="80dp"
                        android:layout_height="100dp"
                        android:src="@drawable/fenge"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="5">
                    <TextView
                        android:id="@+id/zonghe"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="综合评级"
                        android:textColor="#000"
                        android:layout_centerHorizontal="true"
                        android:textSize="20sp"
                        android:layout_marginTop="10dp"/>
                    <TextView
                        android:id="@+id/pjJudge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="B+"
                        android:textColor="#138BA9"
                        android:layout_below="@+id/zonghe"
                        android:layout_centerHorizontal="true"
                        android:textSize="30sp"
                        />
                </RelativeLayout>
            </LinearLayout>


            <!--成绩栏-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/score"
                    android:layout_marginTop="5dp">
                    <TextView
                        android:id="@+id/englishAnnounce"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="大学英语"
                        android:textSize="25sp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/enslighCj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="成绩:"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_below="@+id/englishAnnounce"
                        android:layout_marginTop="5dp"/>
                    <TextView
                        android:id="@+id/jd1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="绩点："
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/englishAnnounce"
                        android:layout_marginTop="5dp"/>
                    <TextView
                        android:id="@+id/englishJ"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00"
                        android:textSize="20sp"
                        android:layout_below="@id/englishAnnounce"
                        android:layout_toRightOf="@+id/jd1"
                        android:layout_marginTop="5dp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/englishJudge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="B+"
                        android:layout_marginRight="10dp"
                        android:textSize="30sp"
                        android:textColor="#0096BB"
                        android:layout_alignParentRight="true"/>
                    <TextView
                        android:id="@+id/englishP"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/enslighCj"
                        android:layout_below="@+id/englishAnnounce"
                        android:textSize="20sp"
                        android:text="00"
                        android:textColor="#000000"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/score"
                    android:layout_marginTop="15dp">
                    <TextView
                        android:id="@+id/mathAnnounce"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="高等数学"
                        android:textSize="25sp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/mathCj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="成绩:"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_below="@+id/mathAnnounce"
                        android:layout_marginTop="5dp"/>
                    <TextView
                        android:id="@+id/mathP"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/mathCj"
                        android:layout_below="@+id/mathAnnounce"
                        android:textSize="20sp"
                        android:text="00"
                        android:textColor="#000000"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"/>
                    <TextView
                        android:id="@+id/jd2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="绩点："
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/mathAnnounce"
                        android:layout_marginTop="5dp"/>
                    <TextView
                        android:id="@+id/mathJ"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00"
                        android:textSize="20sp"
                        android:layout_below="@id/mathAnnounce"
                        android:layout_toRightOf="@+id/jd2"
                        android:layout_marginTop="5dp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/mathJudge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="A-"
                        android:layout_marginRight="10dp"
                        android:textSize="30sp"
                        android:textColor="#0096BB"
                        android:layout_alignParentRight="true"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/score"
                    android:layout_marginTop="15dp">
                    <TextView
                        android:id="@+id/thchniqueAnnounce"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="技术导论"
                        android:textSize="25sp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/techniqueCj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="成绩:"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_below="@+id/thchniqueAnnounce"
                        android:layout_marginTop="5dp"/>
                    <TextView
                        android:id="@+id/jd3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="绩点："
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_marginTop="5dp"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/thchniqueAnnounce" />
                    <TextView
                        android:id="@+id/techniqueJ"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00"
                        android:textSize="20sp"
                        android:layout_below="@id/thchniqueAnnounce"
                        android:layout_toRightOf="@+id/jd3"
                        android:layout_marginTop="5dp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/techniqueJudge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="A+"
                        android:layout_marginRight="10dp"
                        android:textSize="30sp"
                        android:textColor="#0096BB"
                        android:layout_alignParentRight="true"/>
                    <TextView
                        android:id="@+id/techniqueP"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/techniqueCj"
                        android:layout_below="@+id/thchniqueAnnounce"
                        android:textSize="20sp"
                        android:text="00"
                        android:textColor="#000000"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/score"
                    android:layout_marginTop="15dp">
                    <TextView
                        android:id="@+id/computerAnnounce"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="程序设计"
                        android:textSize="25sp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/computerCj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="成绩:"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_below="@+id/computerAnnounce"
                        android:layout_marginTop="5dp"/>
                    <TextView
                        android:id="@+id/jd4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="绩点："
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_marginTop="5dp"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/computerAnnounce" />
                    <TextView
                        android:id="@+id/computerJ"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="00"
                        android:textSize="20sp"
                        android:layout_below="@id/computerAnnounce"
                        android:layout_toRightOf="@+id/jd4"
                        android:layout_marginTop="5dp"
                        android:textColor="#000000"/>
                    <TextView
                        android:id="@+id/computerJudge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="SS"
                        android:layout_marginRight="10dp"
                        android:textSize="30sp"
                        android:textColor="#0096BB"
                        android:layout_alignParentRight="true"/>
                    <TextView
                        android:id="@+id/computerP"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/computerCj"
                        android:layout_below="@+id/computerAnnounce"
                        android:textSize="20sp"
                        android:text="00"
                        android:textColor="#000000"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="5dp"/>
                </RelativeLayout>


            </LinearLayout>


        </LinearLayout>


    </ScrollView>

</LinearLayout>